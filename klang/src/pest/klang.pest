WHITESPACE = _{ " " | "\t" }
COMMENT = _{
    "//" ~ (!("\n" | "\r") ~ ANY)* ~ ("\r"? ~ "\n" | EOI) |
    "/*" ~ (!"*/" ~ ANY)* ~ "*/"
}

text = { ( ASCII_ALPHANUMERIC | "," | "." | "!" | "?" | "%" | " " | "[" | "]" )+ }
command = { text }
function = { ">" ~ text ~ "{" ~ line* ~ "}" }
function_call = { "\"" ~ text }
empty_line = { NEWLINE }
line = { function | function_call | command | empty_line }
program = { SOI ~ line* ~ EOI }
